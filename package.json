{
  "name": "feedback-sentiment-platform",
  "version": "2.0.0",
  "description": "Multi-entity async feedback sentiment analysis platform with BullMQ and Redis",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "worker": "ts-node-dev --respawn --transpile-only src/queue/worker.ts"
  },
  "dependencies": {
    "bullmq": "^5.7.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.3.2",
    "sentiment": "^5.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/sentiment": "^5.0.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
